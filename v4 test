	if assets.Music.Playing then
		assets.Music.Playing = false
		home.Music.SongController.PlayIcon.Visible = true
		home.Music.SongController.PauseIcon.Visible = false
	else
		repStor.RemoteEvent:FireServer(unpack({[1] = {[1] = "Setting",[2] = "BGMusic",[3] = false}}))
		assets.Music.Playing = true
		home.Music.SongController.PlayIcon.Visible = false
		home.Music.SongController.PauseIcon.Visible = true
	end
end)
home.Music.Playlist.ScrollingFrame.Input.ButtonFrame.Button.MouseButton1Click:Connect(function()
	repStor.RemoteEvent:FireServer(unpack({[1] = {[1] = "Setting",[2] = "BGMusic",[3] = false}}))
	local asset = home.Music.Playlist.ScrollingFrame.Input.InputFrame.AssetBox
	assets.Music.SoundId = "rbxassetid://"..tonumber(asset.Text)
	home.Music.SongName.Text = marketService:GetProductInfo(tostring(string.gsub(assets.Music.SoundId, "rbxassetid://", ""))).Name
	asset.Text = ""
	assets.Music.Playing = true
	assets.Music.TimePosition = 0
	home.Music.SongController.PlayIcon.Visible = false
	home.Music.SongController.PauseIcon.Visible = true
end)

if UserJoinLeave == true then
	WhatIsCurrentlyLoading = "Ceating User Join/Leave Notifications"
end

Players.PlayerAdded:Connect(function(plr)
	if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
		if UserJoinLeave == true then
			repeat wait() until HasFullyExecuted == true
			if player:IsFriendsWith(plr.UserId) then
				newNotification("Friend Joined", "Join", plr.Name.." has joined this Server", 10)
			else
				newNotification("User Joined", "Join", plr.Name.." has joined this Server", 10)
			end
		end
	end
end)

Players.PlayerRemoving:Connect(function(plr)
	if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
		if UserJoinLeave == true then
			repeat wait() until HasFullyExecuted == true
			if player:IsFriendsWith(plr.UserId) then
				newNotification("Friend Left", "Leave", plr.Name.." has left this Server", 10)
			else
				newNotification("User Left", "Leave", plr.Name.." has left this Server", 10)
			end
		end
	end
end)

local function tweenColor(object, color, tweenTime)
	tweenServ:Create(object, TweenInfo.new(tweenTime, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0), {BackgroundColor3 = color}):Play()
end

