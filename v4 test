if SoundPlaying == false then
	home.Music.SongController.PlayIcon.Visible = true
	home.Music.SongController.PauseIcon.Visible = false
end

spawn(function()
	while wait(0.1) do
		if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then break end
		SoundId = InsertedObjects.Music.SoundId
		SoundPlaying = InsertedObjects.Music.Playing
		SoundTimePosition = InsertedObjects.Music.TimePosition
		UpdateSaves()
	end
end)

WhatIsCurrentlyLoading = "Finishing Script"
HasFullyExecuted = true
FullyLoaded = true

if Blacklisted == "false" then
	yx.MainController.Visible = true
	assets.Music.Playing = SoundPlaying
end
end)

----------------------------------------

local function roundNumber(num, numDecimalPlaces)
	return tonumber(string.format("%." .. (numDecimalPlaces or 0) .. "f", num))
  end

spawn(function()
	repeat wait() until WaitForNotification == false
	newNotification("Loading", "Loading", "Yukii X is currently being loaded", 20)
end)

spawn(function()
repeat wait() until FullyLoaded == true
FullyLoaded = "Stop"
spawn(function()
	if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber).NotificationHud:FindFirstChild("Loading") then
		game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber).NotificationHud:FindFirstChild("Loading"):Destroy()
	end
end)

if HasFullyExecuted == true then
	repeat wait() until game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber); repeat wait() until game:GetService("ReplicatedStorage"):FindFirstChild("YX_Assets")
		if Blacklisted == "true" then
			newNotification("Blacklisted", "Leave", "You are blacklisted from using Yukii X", 3)
			wait(3)
			game.Players.LocalPlayer:Kick("Blacklisted")
		elseif HasPremium == "true" then
			newNotification("Started", "Premium", "Yukii X 4.0 has loaded fully!\nStartup Time: "..roundNumber(StartupTime, 1).." Seconds\nPremium Active", 10)
		elseif HasPremium == "false" then
			newNotification("Started", "Update", "Yukii X 4.0 has loaded fully!\nStartup Time: "..roundNumber(StartupTime, 1).." Seconds\nPlease consider buying Premium for extra Features!", 10)
		end
		if _G.lastHud then
			print(_G.lastHud)
			tweenHud(_G.lastHud, true)
			visibleHud = _G.lastHud
		end
	else
		newNotification("Error", "Disconnected", "Yukii X couldn't load fully, please report this bug or try rejoining", 10)
		wait(5)
		spawn(function()
			if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
				game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber):Destroy()
			end
		end)
		if game:GetService("ReplicatedStorage"):FindFirstChild("YX_Assets") then
			game:GetService("ReplicatedStorage"):FindFirstChild("YX_Assets"):Destroy()
		end
	end
end)

local function endAllProcesses()
	--// Global
	_G.lastHud = visibleHud
	tweenOthers(nil)

	--// PlayerHud
	yx.PeopleHud.UserStatsFrame.Visible = false
	yx.PeopleHud.UserActionFrame.Visible = false

end

spawn(function()
	if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
		endAllProcesses()
	end
end)


wait(20)
if FullyLoaded ~= "Stop" then
	FullyLoaded = "Stop"
	if HasFullyExecuted == true then
		repeat wait() until game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber); repeat wait() until game:GetService("ReplicatedStorage"):FindFirstChild("YX_Assets")
		if Blacklisted == "true" then
			newNotification("Blacklisted", "Leave", "You are blacklisted from using Yukii X", 3)
			wait(3)
			game.Players.LocalPlayer:Kick("Blacklisted")
		elseif HasPremium == "true" then
			newNotification("Started", "Premium", "Yukii X 4.0 has loaded fully! Startup Time: "..roundNumber(StartupTime, 1).." Seconds\nPremium Active", 10)
		elseif HasPremium == "false" then
			newNotification("Started", "Update", "Yukii X 4.0 has loaded fully! Startup Time: "..roundNumber(StartupTime, 1).." Seconds\nPlease consider buying Premium for extra Features!", 10)
		end
	else
		newNotification("Error", "Disconnected", "Yukii X couldn't load fully, please report this bug or try rejoining", 10)
		wait(5)
		spawn(function()
			if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
				game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber):Destroy()
			end
		end)
		if game:GetService("ReplicatedStorage"):FindFirstChild("YX_Assets") then
			game:GetService("ReplicatedStorage"):FindFirstChild("YX_Assets"):Destroy()
		end
	end
end
