if not getgenv().RandomNumber then
	getgenv().RandomNumber = math.random(0, 1000000000)
end


if game.CoreGui:FindFirstChild("Yukii X" .. RandomNumber) then
	WhatIsCurrentlyLoading = "Removing old Yukii X"
	game.CoreGui:FindFirstChild("Yukii X" .. RandomNumber):Destroy()
	if game:GetService("ReplicatedStorage"):FindFirstChild("YX_Assets") then
	game:GetService("ReplicatedStorage"):FindFirstChild("YX_Assets"):Destroy()
	end
end

getgenv().RandomNumber = math.random(0, 1000000000)
local KeepRandomNumber = RandomNumber

spawn(function()
-----------------------------------------------------
--Data
WhatIsCurrentlyLoading = "Getting Data"
local function LoadDefaults()
	UserJoinLeave = true
	Noclip = false
	SoundId = "rbxassetid://5428202129"
	SoundPlaying = true
	SoundTimePosition = 0
	SoundVolume = 0.1
	ReturnOnDeath = false
	SuperAura = false
	NoRepTake = false
	NoVoid = false
	RainbowRank = false
	RainbowWeights = false
	OpenKeybind = "K"
	DarkModeAutoExe = false
	BlackHubAutoExe = false
	ChatBypassAutoExe = false
	LastPosition = false
	BetterEspAutoExe = false
	SuggestionsWait = 0
	CmdXAutoExe = false
	InfiniteYieldAutoExe = false
	TimeFormat = "military"
	CBringAutoExe = false
end

local function UpdateSaves()
    local Update = {
	UserJoinLeave = UserJoinLeave,
	Noclip = Noclip,
	SoundId = SoundId,
	SoundPlaying = SoundPlaying,
	SoundTimePosition = SoundTimePosition,
	SoundVolume = SoundVolume,
	ReturnOnDeath = ReturnOnDeath,
	SuperAura = SuperAura,
	NoRepTake = NoRepTake,
	NoVoid = NoVoid,
	RainbowRank = RainbowRank,
	RainbowWeights = RainbowWeights,
	OpenKeybind = OpenKeybind,
	DarkModeAutoExe = DarkModeAutoExe,
	BlackHubAutoExe = BlackHubAutoExe,
	ChatBypassAutoExe = ChatBypassAutoExe,
	LastPosition = LastPosition,
	BetterEspAutoExe = BetterEspAutoExe,
	SuggestionsWait = SuggestionsWait,
	CmdXAutoExe = CmdXAutoExe,
	InfiniteYieldAutoExe = InfiniteYieldAutoExe,
	TimeFormat = TimeFormat,
	CBringAutoExe = CBringAutoExe,
    }
    writefile("YukiiXV4Data.lua", game:GetService("HttpService"):JSONEncode(Update))
end

local function LoadSaves()
    local success, errorsend =
        pcall(
        function()
			WhatIsCurrentlyLoading = "Data Found"
            saves = game:GetService("HttpService"):JSONDecode(readfile("YukiiXV4Data.lua"))
        end
    )
    if not success then
		WhatIsCurrentlyLoading = "Creating Data"
        LoadDefaults()
        UpdateSaves()
        return
    end
	UserJoinLeave = saves.UserJoinLeave
	Noclip = saves.Noclip
	SoundId = saves.SoundId
	SoundPlaying = saves.SoundPlaying
	SoundTimePosition = saves.SoundTimePosition
	SoundVolume = saves.SoundVolume
	ReturnOnDeath = saves.ReturnOnDeath
	SuperAura = saves.SuperAura
	NoRepTake = saves.NoRepTake
	NoVoid = saves.NoVoid
	RainbowRank = saves.RainbowRank
	RainbowWeights = saves.RainbowWeights
	OpenKeybind = saves.OpenKeybind
	DarkModeAutoExe = saves.DarkModeAutoExe
	BlackHubAutoExe = saves.BlackHubAutoExe
	ChatBypassAutoExe = saves.ChatBypassAutoExe
	LastPosition = saves.LastPosition
	BetterEspAutoExe = saves.BetterEspAutoExe
	SuggestionsWait = saves.SuggestionsWait
	CmdXAutoExe = saves.CmdXAutoExe
	InfiniteYieldAutoExe = saves.InfiniteYieldAutoExe
	TimeFormat = saves.TimeFormat
	CBringAutoExe = saves.CBringAutoExe
end

if writefile and readfile then
    LoadSaves()
else
    LoadDefaults()
end

