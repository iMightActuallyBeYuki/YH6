	if result ~= 0 and result < 11 then
		result = 11
	end
	lph.ActionFrame.ActionSlideFrame.SpeedSliderFrame.SpeedSlider.SpeedLabel.Text = gameSettings.SettingTxt15.TextBox.Text.." Movement Speed"
