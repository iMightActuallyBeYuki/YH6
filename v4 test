local HasFullyExecuted = false
local FullyLoaded = false
local WaitForNotification = true
local StartupTime = 0
local WhatIsCurrentlyLoading = ""

spawn(function()
	while wait(0.1) do
		if HasFullyExecuted == false then
		StartupTime += 0.1
		end
	end
end)

local HasPremium
local Authorized
local Blacklisted

WhatIsCurrentlyLoading = "Getting Whitelist"

local YarisConfig = {
    ['API_KEY'] =  "rqPA49kLenDg8EvamTGt";
    ['HWID'] =  "";
}

local YarisConfigPremium = {
    ['API_KEY'] =  "UVugEKjnGW6YaL4ybMXe";
    ['HWID'] =  "";
}

local http_request = http_request;

if syn then
    http_request = syn.request
end

local body = http_request({Url = 'https://httpbin.org/get'; Method = 'GET'}).Body;
local decoded = game:GetService('HttpService'):JSONDecode(body)
local hwid_list = {"Syn-Fingerprint", "Exploit-Guid", "Krnl-Hwid"};
for i, v in next, hwid_list do
    if decoded.headers[v] then
        YarisConfig.HWID = decoded.headers[v];
        YarisConfigPremium.HWID = decoded.headers[v];
        break
    end
end

local JoinLink = "https://www.roblox.com/home?placeID="..game.PlaceId.."&gameID="..game.JobId

local auth = game:HttpGet('https://yaris.rocks/access/?apikey='..YarisConfig.API_KEY..'&hwid='..YarisConfig.HWID .."&disable=true")
local auth2 = game:HttpGet('https://yaris.rocks/access/?apikey='..YarisConfigPremium.API_KEY..'&hwid='..YarisConfigPremium.HWID .."&disable=true")


if string.find(auth2, '"blacklisted": true') or string.find(auth, '"blacklisted": true') then
	Blacklisted = "true"
	Authorized = "false"
	HasPremium = "false"
elseif string.find(auth2, '"blacklisted":false') then
	Authorized = "true"
	Blacklisted = "false"
	HasPremium = "true"
elseif string.find(auth, '"blacklisted":false') then
	Authorized = "true"
	Blacklisted = "false"
	HasPremium = "false"
else
	Authorized = "false"
	Blacklisted = "false"
	HasPremium = "false"
end

local color

if HasPremium == "true" then
    color = "0x7269da"
elseif Authorized == "true" and HasPremium == "false" then
    color = "0x00FF00"
elseif Blacklisted == "true" then
    color = "0xff0000"
elseif Authorized == "false" then
	color = "black"
end

