if LastPosition ~= false then
	local char = player.Character or player.CharacterAdded:Wait()
	local hrp = char:WaitForChild("HumanoidRootPart")

	WhatIsCurrentlyLoading = "Teleporting back to old position"
	hrp.CFrame = CFrame.new(LastPosition.X, LastPosition.Y, LastPosition.Z)
	LastPosition = false
end

local function getPlayer(input)
	for i, v in pairs(Players:GetPlayers()) do
		if string.match(string.lower(v.Name), string.lower(input)) then return v end
		if string.match(string.lower(v.DisplayName), string.lower(input)) then return v end
	end
end

WhatIsCurrentlyLoading = "Creating Tweens"
local function tweenControllerButton(button, bool)
	local tweenTime = 0.375
	local colors = {
		["Enable"] = {
			Color3.fromRGB(255, 255, 255),
			Color3.fromRGB(61, 61, 61),
			Color3.fromRGB(61, 61, 61)
		},
		["Disable"] = {
			Color3.fromRGB(61, 61, 61),
			Color3.fromRGB(255, 255, 255),
			Color3.fromRGB(255, 255, 255)
		}
	}
	if button.Name == "PremiumFrame" then
		colors = {
			["Enable"] = {
				Color3.fromRGB(255, 145, 90),
				Color3.fromRGB(61, 61, 61),
				Color3.fromRGB(61, 61, 61)
			},
			["Disable"] = {
				Color3.fromRGB(61, 61, 61),
				Color3.fromRGB(255, 175, 120),
				Color3.fromRGB(255, 145, 90)
			}
		}
	end
	if bool then
		tweenServ:Create(button, TweenInfo.new(
			tweenTime,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
			), {
				BackgroundColor3 = colors.Enable[1]
			}):Play()
		tweenServ:Create(button.Stroke, TweenInfo.new(
			tweenTime,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
			), {
				Color = colors.Enable[2]
			}):Play()
		tweenServ:Create(button.Icon, TweenInfo.new(
			tweenTime,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
			), {
				ImageColor3 = colors.Enable[3]
			}):Play()
	else
		tweenServ:Create(button, TweenInfo.new(
			tweenTime / 2,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
			), {
				BackgroundColor3 = colors.Disable[1]
			}):Play()
		tweenServ:Create(button.Stroke, TweenInfo.new(
			tweenTime / 2,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
			), {
				Color = colors.Disable[2]
			}):Play()
		tweenServ:Create(button.Icon, TweenInfo.new(
			tweenTime / 2,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
			), {
				ImageColor3 = colors.Disable[3]
			}):Play()
	end
end

local function transparencyTween(gui, tweenTime, t, bool)
	spawn(function()
		if bool then
			gui.Visible = true
			for i, object in pairs(gui:GetDescendants()) do
				if not object:IsA("UICorner") and not object:IsA("UIListLayout") and not object:IsA("UIGradient") then
					if object:IsA("ImageLabel") or object:IsA("ImageButton") then
						for i, v in pairs(t) do
							if v[1] == object then
								tweenServ:Create(object, TweenInfo.new(
									tweenTime, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0), {
										ImageTransparency = v[2]
									}):Play()
								break
							end
						end
					elseif object:IsA("TextLabel") or object:IsA("TextButton") or object:IsA("TextBox") then
						for i, v in pairs(t) do
							if v[1] == object then
								tweenServ:Create(object, TweenInfo.new(
									tweenTime, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0), {
										TextTransparency = v[2]
									}):Play()
								break
							end
						end
					else
						for i, v in pairs(t) do
							if v[1] == object then
								tweenServ:Create(object, TweenInfo.new(
									tweenTime, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0), {
										Transparency = v[2]
									}):Play()
								break
							end
						end
					end
				end
			end
			for i, v in pairs(t) do
				if v[1] == gui then
					tweenServ:Create(gui, TweenInfo.new(
						tweenTime, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0), {
							Transparency = v[2]
						}):Play()
					break
				end
			end
			delay(tweenTime, function()
				table.clear(t)
			end)
		else
			for i, object in pairs(gui:GetDescendants()) do
				if not object:IsA("UICorner") and not object:IsA("UIListLayout") and not object:IsA("UIGradient") then
					if object:IsA("ImageLabel") or object:IsA("ImageButton") then
						table.insert(t, {object, object.ImageTransparency})
						tweenServ:Create(object, TweenInfo.new(
							tweenTime, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0), {
								ImageTransparency = 1
							}):Play()
					elseif object:IsA("TextLabel") or object:IsA("TextButton") or object:IsA("TextBox") then
						table.insert(t, {object, object.TextTransparency})
						tweenServ:Create(object, TweenInfo.new(
							tweenTime, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0), {
								TextTransparency = 1
							}):Play()
					else
						if not object:IsA("Configuration") and not object:IsA("UIGridLayout") then
							table.insert(t, {object, object.Transparency})
							tweenServ:Create(object, TweenInfo.new(
								tweenTime, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0), {
									Transparency = 1
								}):Play()
						end
					end
				end
			end
			table.insert(t, {gui, gui.Transparency})
			tweenServ:Create(gui, TweenInfo.new(
				tweenTime, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0), {
					Transparency = 1
				}):Play()
			delay(tweenTime, function()
				gui.Visible = false
			end)
		end
	end)
end

WhatIsCurrentlyLoading = "Creating Notifications"
local notificationQueue = 0
local notifications = {}
function newNotification(name, icon, text, aliveTime)
	--// Icons: Disconnected, Join, Leave, Reconnected, Loading, Update \\--
    local notiFrame = yx:WaitForChild("NotificationHud")

	notificationQueue += 1

	wait(notificationQueue - 1)

	local newNoti = assets.Notification:Clone()
	table.insert(notifications, {newNoti, #yx.NotificationHud:GetChildren() + 1})
	newNoti.Parent = yx.NotificationHud
	newNoti.NotificationTItleFrame.NotificationName.Text = name
	newNoti.NotificationText.Text = text
	newNoti.Name = name
	if newNoti.NotificationTItleFrame:FindFirstChild(icon.."Icon") then
		newNoti.NotificationTItleFrame:FindFirstChild(icon.."Icon").Visible = true
	elseif icon == "Premium" then
		newNoti.NotificationTItleFrame:FindFirstChild("UpdateIcon").Visible = true
	end
	if icon == "Join" then
		newNoti.NotificationStroke.Color = Color3.fromRGB(200, 255, 180)
	elseif icon == "Loading" then
		newNoti.NotificationStroke.Color = Color3.fromRGB(44, 44, 44)
		spawn(function()
			while newNoti do
				if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then break end
				newNoti:WaitForChild("NotificationText").Text = "Yukii X is currently being loaded\n"..WhatIsCurrentlyLoading
				wait(0.1)
			end
		end)

		spawn(function()
				local TweenService = game:GetService("TweenService")
				local Object = newNoti.NotificationTItleFrame.LoadingIcon
				Object.Rotation = 0
				
				local tweenInfo = TweenInfo.new(2,Enum.EasingStyle.Quad,Enum.EasingDirection.Out,20,false,0)
				
				local Tween = TweenService:Create(Object, tweenInfo, {Rotation = 360})
				Tween:Play()
		end)
	elseif icon == "Leave" then
		newNoti.NotificationStroke.Color = Color3.fromRGB(255, 65, 65)
	elseif icon == "Update" then
		newNoti.NotificationStroke.Color = Color3.fromRGB(150, 155, 255)
	elseif icon == "Premium" then
		newNoti.NotificationStroke.Color = Color3.fromRGB(255, 145, 90)
	elseif icon == "Disconnected" then
		spawn(function()
			while newNoti:FindFirstChild("NotificationStroke") do
				if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
				wait(0.5)
				if newNoti:FindFirstChild("NotificationStroke") then
					newNoti.NotificationStroke.Color = Color3.fromRGB(255, 65, 65)
				end
				wait(0.5)
				if newNoti:FindFirstChild("NotificationStroke") then
					newNoti.NotificationStroke.Color = Color3.fromRGB(255, 255, 255)
				end
			end
			end
		end)
	end
	newNoti.Position = UDim2.new(-newNoti.Size.X.Scale, 0, 1, 0)


	if #notiFrame:GetChildren() > 0 then
        for i, noti in pairs(notiFrame:GetChildren()) do
			if not noti then continue end
            noti:TweenPosition(UDim2.new(0.0125, 0, noti.Position.Y.Scale - 0.2025, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, true)
		end
	end

	local notiData = {}

	if not newNoti then return end
	newNoti:TweenPosition(UDim2.new(0.0125, 0, 0.995, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, true)
	delay(aliveTime, function()
		notificationQueue -= 1
		if notificationQueue < 0 then
			notificationQueue = 0
		end
		transparencyTween(newNoti, 0.5, notiData, false)
		delay(0.5, function()
			for i, notif in pairs(notifications) do
				if notif[1] == newNoti then
					table.remove(notifications, i)
					if not newNoti then continue end
					newNoti:Destroy()
					break
				end
			end
			for i, notif in pairs(notifications) do
				if notif[2] == i then
					if not notif[1] then continue end
					if notif[1].Parent == yx.NotificationHud then
						notif[1]:TweenPosition(UDim2.new(0.0125, 0, notif[1].Position.Y.Scale + (0.1975 * (i + 1)), 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, true)
					end
				end
			end
			if #yx:WaitForChild("NotificationHud"):GetChildren() == 1 then
				yx:WaitForChild("NotificationHud"):GetChildren()[1]:TweenPosition(UDim2.new(0.0125, 0, 0.995, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, true)
			end
		end)
	end)
end

WaitForNotification = false

local function homeHudTween(bool)
	local tweenTime = 0.4
	local Friends = yx.HomeHud.Friends
	local Discord = yx.HomeHud.Discord
	local Credits = yx.HomeHud.Credits
	local Keybind = yx.HomeHud.Keybind
	local Music = yx.HomeHud.Music
	local Suggest = yx.HomeHud.Suggestions

	if bool then
		yx.HomeHud.Visible = true
		if not game.Lighting.Blur then
			local blur = Instance.new("BlurEffect")
			blur.Parent = game.Lighting
		end
		game.Lighting.Blur.Enabled = true
		tweenServ:Create(game.Lighting.Blur, TweenInfo.new(
			tweenTime * 1.5,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
			), {
				Size = 10
			}):Play()
		tweenServ:Create(workspace.CurrentCamera, TweenInfo.new(
			tweenTime * 1.5,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
			), {
				FieldOfView = 30
			}):Play()
		Credits:TweenPosition(UDim2.new(0.335, 0, 0.324, 0), Enum.EasingDirection.In, Enum.EasingStyle.Bounce, tweenTime, true)
		wait(tweenTime / 5)
		Keybind:TweenPosition(UDim2.new(0.335, 0, 0.566, 0), Enum.EasingDirection.In, Enum.EasingStyle.Bounce, tweenTime, true)
		wait(tweenTime / 5)
		Discord:TweenPosition(UDim2.new(0.125, 0, 0.274, 0), Enum.EasingDirection.In, Enum.EasingStyle.Bounce, tweenTime, true)
		Suggest:TweenPosition(UDim2.new(0.875, 0, 0.324, 0), Enum.EasingDirection.In, Enum.EasingStyle.Bounce, tweenTime, true)
		wait(tweenTime / 5)
		Friends:TweenPosition(UDim2.new(0.125, 0, 0.656, 0), Enum.EasingDirection.In, Enum.EasingStyle.Bounce, tweenTime, true)
		Music:TweenPosition(UDim2.new(0.875, 0, 0.823, 0), Enum.EasingDirection.In, Enum.EasingStyle.Bounce, tweenTime, true)
	else
		tweenServ:Create(game.Lighting.Blur, TweenInfo.new(
			tweenTime * 1.5,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
			), {
				Size = 0
			}):Play()
		tweenServ:Create(workspace.CurrentCamera, TweenInfo.new(
			tweenTime * 1.5,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
			), {
				FieldOfView = 70
			}):Play()
		Music:TweenPosition(UDim2.new(1.25, 0, 0.823, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, tweenTime, true)
		Friends:TweenPosition(UDim2.new(-0.25, 0, 0.656, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, tweenTime, true)
		wait(tweenTime / 5)
		Suggest:TweenPosition(UDim2.new(1.25, 0, 0.324, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, tweenTime, true)
		Discord:TweenPosition(UDim2.new(-0.25, 0, 0.274, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, tweenTime, true)
		wait(tweenTime / 5)
		Keybind:TweenPosition(UDim2.new(-0.25, 0, 0.566, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, tweenTime, true)
		wait(tweenTime / 5)
		Credits:TweenPosition(UDim2.new(-0.25, 0, 0.324, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, tweenTime, true)
		delay(tweenTime, function()
			yx:WaitForChild("HomeHud").Visible = false
		end)
	end
end

local function playerHudTween(bool)
	spawn(function()
		local tweenTime = 0.2
		local frame = yx.PlayerHud

		transparencyTween(frame, tweenTime, playerHudData, bool)
	end)
end

local function peopleHudTween(bool)
	spawn(function()
		local tweenTime = 0.2
		local frame = yx.PeopleHud

		transparencyTween(frame, tweenTime, peopleHudData, bool)
	end)
end

local function miscHudTween(bool)
	spawn(function()
		local tweenTime = 0.2
		local frame = yx.MiscHud

		transparencyTween(frame, tweenTime, miscHudData, bool)
	end)
end

local function premiumHudTween(bool)
	spawn(function()
		local tweenTime = 0.2
		local frame = yx.PremiumPurchaseHud

		if HasPremium == "true" then
			frame = yx.PremiumHud
        else
            if yx:FindFirstChild("PremiumHud") then
				yx.PremiumHud:Destroy()
			end
        end
        transparencyTween(frame, tweenTime, premiumHudData, bool)
	end)
end

yx.PremiumPurchaseHud.Frame.Purchasing.Purchase.Button.MouseButton1Click:Connect(function()
    setclipboard("https://www.roblox.com/catalog/7512378939/Premium-Red")
    yx.PremiumPurchaseHud.Frame.PurchaseInfo.Visible = true
    delay(5, function()
        yx.PremiumPurchaseHud.Frame.PurchaseInfo.Visible = false
    end)
end)

homeHudTween(false)
playerHudTween(false)
peopleHudTween(false)
miscHudTween(false)
premiumHudTween(false)

transparencyTween(yx.ToggleButton, 0.2, toggleButtonData, false)

local function tweenHud(hud, bool)
	if hud == "Home" then
		homeHudTween(bool)
	elseif hud == "Player" then
		playerHudTween(bool)
	elseif hud == "People" then
		peopleHudTween(bool)
	elseif hud == "Misc" then
		miscHudTween(bool)
	elseif hud == "Premium" then
		premiumHudTween(bool)
	end
end

local function tweenOthers(hud)
	if hud ~= "Home" then
		if yx.HomeHud.Discord.Position.X ~= UDim.new(-0.25, 0) then
			homeHudTween(false)
		end
	end
	if hud ~= "Player" then
		if yx.PlayerHud.Transparency == 0 then
			playerHudTween(false)
		end
	end
	if hud ~= "People" then
		if yx.PeopleHud.Transparency == 0 then
			peopleHudTween(false)
		end
	end
	if hud ~= "Misc" then
		if yx.MiscHud.Transparency == 0 then
			miscHudTween(false)
		end
	end
	if hud ~= "Premium" then
		if yx:FindFirstChild("PremiumHud") and yx.PremiumHud.Transparency == 0 or yx.PremiumPurchaseHud.Transparency == 0 then
			premiumHudTween(false)
		end
	end
end

local ScriptButtonScale = 0
local function newScript(name, description, color, isToggle, callback)
	local ui = assets.ScriptButton:Clone()
	yx.MiscHud.ActionFrame.ScrollingFrame.UIListLayout.Padding = UDim.new(0, 5)
	if isToggle then
		ui.Execute.TextLabel.Text = "Toggle"
	end
	ui.Parent = yx.MiscHud.ActionFrame.ScrollingFrame
	ui.Title.Text = name
	if ScriptButtonScale == 0 then ScriptButtonScale = ui.AbsoluteSize.Y end
	ui.Size = UDim2.new(ui.Size.X.Scale, ui.Size.X.Offset, 0, ScriptButtonScale)
	ui.Description.Text = description
	ui.BackgroundColor3 = color
	yx.MiscHud.ActionFrame.ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, yx.MiscHud.ActionFrame.ScrollingFrame.UIListLayout.AbsoluteContentSize.Y + 2.5)

	local autoExe = callback(nil, false, true)
	if autoExe then
		ui.AutoExe.Frame.Icon.Visible = true
		autoExe = true
		callback(autoExe, true, false)
	else
		ui.AutoExe.Frame.Icon.Visible = false
		autoExe = false
	end
	ui.AutoExe.Frame.Button.MouseButton1Click:Connect(function()
		autoExe = not autoExe
		if autoExe then
			ui.AutoExe.Frame.Icon.Visible = true
			autoExe = true
			callback(autoExe, false, false)
		else
			ui.AutoExe.Frame.Icon.Visible = false
			autoExe = false
			callback(autoExe, false, false)
		end
	end)
	ui.Execute.Button.MouseButton1Click:Connect(function()
		if not isToggle then
			callback(autoExe, true, false, true)
		else
			callback(autoExe, true, false, true)
		end
	end)
end

Players.PlayerRemoving:Connect(function(plr)
	if plr == player then
		_G.darkMenu = false
		_G.blackHub = false
		_G.chatBypass = false
		_G.betterEsp = false
		_G.CmdX = false
		_G.InfY = false
		_G.CBring = false
	end
end)

WhatIsCurrentlyLoading = "Creating ESP"
local Plrs = game:GetService("Players")
local plr = game:GetService("Players").LocalPlayer
local Run = game:GetService("RunService")
local MyPlr = Plrs.LocalPlayer
local MyChar = MyPlr.Character
local CoreGui = game:GetService("CoreGui")
local char = plr.Character
ESPLength = 20000

function round(num, numDecimalPlaces)
	local mult = 10^(numDecimalPlaces or 0)
	return math.floor(num * mult + 0.5) / mult
end

function converttoletter(num)
	if num / 1e21 >=1 then
		newnum = num / 1e21
		return round(newnum, 6).. "Sx"
	elseif num / 1e18 >=1 then
		newnum = num / 1e18
		return round(newnum, 6).. "Qi"
	elseif num / 1e15 >=1 then
		newnum = num / 1e15
		return round(newnum, 6).. "Qa"
	elseif num / 1e12 >=1 then
		newnum = num / 1e12
		return round(newnum, 6).. "T"
	elseif num / 1e09 >=1 then
		newnum = num / 1e09
		return round(newnum, 6).. "B"
	elseif num / 1e06 >=1 then
		newnum = num / 1e06
		return round(newnum, 6).. "M"
	elseif num / 1e03 >=1 then
		newnum = num / 1e03
		return round(newnum, 6).. "K"
	else return num
	end
end

CharAddedEvent = { }
-- local JustJoined

Plrs.PlayerAdded:connect(function(plr)
	if CharAddedEvent[plr.Name] == nil then
		CharAddedEvent[plr.Name] = plr.CharacterAdded:connect(function(char)
			if ESPEnabled then
				RemoveESP(plr)
				CreateESP(plr)
			end
		end)
	end
end)

Plrs.PlayerRemoving:connect(function(plr)
	if CharAddedEvent[plr.Name] ~= nil then
		CharAddedEvent[plr.Name]:Disconnect()
		CharAddedEvent[plr.Name] = nil
	end
	RemoveESP(plr)
end)

-- and v ~= JustJoined

local WaitForCharacterCO = coroutine.create(function()
while wait(2) do
for i,v in pairs(Plrs:GetChildren()) do
	repeat wait() until v.Character
	if v ~= MyPlr and v.Character.Humanoid.Health == 0 then
		if CharAddedEvent[v.Name] == nil then
			CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
			repeat wait() until Char:FindFirstChild("HumanoidRootPart")
			if ESPEnabled then
				RemoveESP(v)
				CreateESP(v)
			end
			end)
		end
	end
end
end
end)

local WaitForCharacterCO2 = coroutine.create(function()
	while wait(2) do
	for i,v in pairs(Plrs:GetChildren()) do
		if v ~= MyPlr and v.Character then
			if CharAddedEvent[v.Name] == nil then
				CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
				repeat wait() until Char:FindFirstChild("HumanoidRootPart")
				if ESPEnabled then
					RemoveESP(v)
					CreateESP(v)
				end
				end)
			end
		end
	end
	end
	end)

coroutine.resume(WaitForCharacterCO)
coroutine.resume(WaitForCharacterCO2)

function CreateESP(plr)
	if plr ~= nil then
		local GetChar = plr.Character
		if not GetChar then return end
		local GetHead do
			repeat wait() until GetChar:FindFirstChild("Head")
		end
		GetHead = GetChar.Head
		
		local bb = Instance.new("BillboardGui", CoreGui)
		bb.Adornee = GetHead
		bb.ExtentsOffset = Vector3.new(0, 1, 0)
		bb.AlwaysOnTop = true
		bb.Size = UDim2.new(0, 5, 0, 5)
		bb.StudsOffset = Vector3.new(0, 3, 0)
		bb.Name = "ESP_" .. plr.Name
		
		local frame = Instance.new("Frame", bb)
		frame.ZIndex = 10
		frame.BackgroundTransparency = 1
		frame.Size = UDim2.new(1, 0, 1, 0)
		
		local TxtName = Instance.new("TextLabel", frame)
		TxtName.Name = "Names"
		TxtName.ZIndex = 10
		TxtName.Text = plr.Name
		TxtName.BackgroundTransparency = 1
		TxtName.Position = UDim2.new(0, 0, 0, -45)
		TxtName.Size = UDim2.new(1, 0, 10, 0)
		TxtName.Font = "SourceSansBold"
		TxtName.TextColor3 = Color3.new(0, 0, 0)
		TxtName.TextSize = 14
		TxtName.TextStrokeTransparency = 0.5
		
		local TxtDist = Instance.new("TextLabel", frame)
		TxtDist.Name = "Dist"
		TxtDist.ZIndex = 10
		TxtDist.Text = ""
		TxtDist.BackgroundTransparency = 1
		TxtDist.Position = UDim2.new(0, 0, 0, -35)
		TxtDist.Size = UDim2.new(1, 0, 10, 0)
		TxtDist.Font = "SourceSansBold"
		TxtDist.TextColor3 = Color3.new(0, 0, 0)
		TxtDist.TextSize = 15
		TxtDist.TextStrokeTransparency = 0.5

		local TxtHealth = Instance.new("TextLabel", frame)
		TxtHealth.Name = "Health"
		TxtHealth.ZIndex = 10
		TxtHealth.Text = ""
		TxtHealth.BackgroundTransparency = 1
		TxtHealth.Position = UDim2.new(0, 0, 0, -25)
		TxtHealth.Size = UDim2.new(1, 0, 10, 0)
		TxtHealth.Font = "SourceSansBold"
		TxtHealth.TextColor3 = Color3.new(0, 0, 0)
		TxtHealth.TextSize = 15
		TxtHealth.TextStrokeTransparency = 0.5

		local TxtFist = Instance.new("TextLabel", frame)
		TxtFist.Name = "Fist"
		TxtFist.ZIndex = 10
		TxtFist.Text = ""
		TxtFist.BackgroundTransparency = 1
		TxtFist.Position = UDim2.new(0, 0, 0, -15)
		TxtFist.Size = UDim2.new(1, 0, 10, 0)
		TxtFist.Font = "SourceSansBold"
		TxtFist.TextColor3 = Color3.new(0, 0, 0)
		TxtFist.TextSize = 15
		TxtFist.TextStrokeTransparency = 0.5

		local TxtPsychic = Instance.new("TextLabel", frame)
		TxtPsychic.Name = "Psychic"
		TxtPsychic.ZIndex = 10
		TxtPsychic.Text = ""
		TxtPsychic.BackgroundTransparency = 1
		TxtPsychic.Position = UDim2.new(0, 0, 0, -5)
		TxtPsychic.Size = UDim2.new(1, 0, 10, 0)
		TxtPsychic.Font = "SourceSansBold"
		TxtPsychic.TextColor3 = Color3.new(0, 0, 0)
		TxtPsychic.TextSize = 15
		TxtPsychic.TextStrokeTransparency = 0.5
	end
end

function UpdateESP(plr)
	local Find = CoreGui:FindFirstChild("ESP_" .. plr.Name)
	if Find then
		local plrStatus = game.Players[plr.Name].leaderstats.Status
		if plrStatus.Value == "Criminal" then
			Find.Frame.Names.TextColor3 = Color3.new(1, 0.1, 1)
		elseif plrStatus.Value == "Lawbreaker" then
			Find.Frame.Names.TextColor3 = Color3.new(1, 0.1, 0.1)
		elseif plrStatus.Value == "Guardian" then
			Find.Frame.Names.TextColor3 = Color3.new(0.1, 0.8, 1)
		elseif plrStatus.Value == "Protector" then
			Find.Frame.Names.TextColor3 = Color3.new(0.1, 0.1, 1)
		elseif plrStatus.Value == "Supervillain" then
			Find.Frame.Names.TextColor3 = Color3.new(0.3, 0.1, 0.1)
		elseif plrStatus.Value == "Superhero" then
			Find.Frame.Names.TextColor3 = Color3.new(0.8, 0.8, 0)
		else
			Find.Frame.Names.TextColor3 = Color3.new(1, 1, 1)
		end
		Find.Frame.Dist.TextColor3 = Color3.new(1, 1, 1)
		Find.Frame.Health.TextColor3 = Color3.new(1, 1, 1)
		Find.Frame.Fist.TextColor3 = Color3.new(1, 1, 1)
		Find.Frame.Psychic.TextColor3 = Color3.new(1, 1, 1)
		local GetChar = plr.Character
		if MyChar and GetChar then
			local Find2 = MyChar:FindFirstChild("HumanoidRootPart")
			local Find3 = GetChar:FindFirstChild("HumanoidRootPart")
			local Find4 = GetChar:FindFirstChildOfClass("Humanoid")
			if Find2 and Find3 then
				local pos = Find3.Position
				local Dist = (Find2.Position - pos).magnitude
				if Dist > ESPLength then
					Find.Frame.Names.Visible = false
					Find.Frame.Dist.Visible = false
					Find.Frame.Health.Visible = false
					Find.Frame.Fist.Visible = false
					Find.Frame.Psychic.Visible = false
					return
				else
					Find.Frame.Names.Visible = true
					Find.Frame.Dist.Visible = true
					Find.Frame.Health.Visible = true
					Find.Frame.Fist.Visible = true
					Find.Frame.Psychic.Visible = true
				end
				Find.Frame.Dist.Text = "Distance: " .. string.format("%.0f", Dist)
				--Find.Frame.Pos.Text = "(X: " .. string.format("%.0f", pos.X) .. ", Y: " .. string.format("%.0f", pos.Y) .. ", Z: " .. string.format("%.0f", pos.Z) .. ")"
				if Find4 then
					Find.Frame.Health.Text = "Health: " ..converttoletter(string.format("%.0f", Find4.Health))
					Find.Frame.Fist.Text = "Fist: " ..converttoletter(string.format("%.0f", game.Players[plr.Name].PrivateStats.FistStrength.Value))
					Find.Frame.Psychic.Text = "Psychic: " ..converttoletter(string.format("%.0f", game.Players[plr.Name].PrivateStats.PsychicPower.Value))
				else
					Find.Frame.Health.Text = ""
					Find.Frame.Fist.Text = ""
					Find.Frame.Psychic.Text = ""
				end
			end
		end
	end
end

function RemoveESP(plr)
	local ESP = CoreGui:FindFirstChild("ESP_" .. plr.Name)
	if ESP then
		ESP:Destroy()
	end
end

spawn(function()
while wait() do
    for i,v in pairs(Plrs:GetPlayers()) do
        UpdateESP(v)
    end
end
end)

local ESPLength = Instance.new("TextBox")

Run:BindToRenderStep("UpdateESP", Enum.RenderPriority.Character.Value, function()
	for _, v in next, Plrs:GetPlayers() do
		UpdateESP(v)
	end
end)

newScript("Better Esp", "View all players Stats and see their Position through walls", Color3.fromRGB(225, 130, 255), true, function(value, exe, isCheck, isForce)
	if not exe and not isCheck then BetterEspAutoExe = value end
	if isCheck and not exe then return BetterEspAutoExe end
	if exe and not isCheck then
		if not _G.betterEsp or isForce then
			_G.betterEsp = not _G.betterEsp

			--------------------------------------------

			UpdateSaves()
			if _G.betterEsp then
				for _, v in next, Plrs:GetPlayers() do
					if v ~= MyPlr and v.Character then
						if v.Character.Humanoid.Health ~= 0 then
						-- if CharAddedEvent[v.Name] ~= nil then
								if _G.betterEsp then
									RemoveESP(v)
									CreateESP(v)
								end
							-- elseif CharAddedEvent[v.Name] == nil then
							-- CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
							-- repeat wait() until Char:FindFirstChild("HumanoidRootPart")
							-- 	if _G.betterEsp then
							-- 		RemoveESP(v)
							-- 		CreateESP(v)
							-- 	end
							-- end)
						-- end
						-- RemoveESP(v)
						-- CreateESP(v)
					end
				end
				end
			else
				for _, v in next, Plrs:GetPlayers() do
					RemoveESP(v)
				end
			end
			--------------------------------------------
		end
	end
end)

