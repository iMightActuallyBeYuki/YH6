local lph = yx.PlayerHud
local gameSettings = pGui.ScreenGui.MenuFrame.SettingFrame
lph.ActionFrame.ActionSlideFrame.SpeedSliderFrame.SpeedSlider.Filling.Size = UDim2.new(gameSettings.SettingTxt15.TextBox.Text / 1000, 0, 1, 0)
