local function hoverInfo(button, infoText, color)
	local infoEl = yx.InfoElement
	if not color then color = infoEl.Background.BackgroundColor3 end
	
	button.MouseEnter:Connect(function()
		infoEl.Visible = true
		infoEl.Background.BackgroundColor3 = color
		infoEl.Background.Bottom.BackgroundColor3 = color
		infoEl.Background.InfoLabel.Text = infoText
		infoEl.Position = UDim2.new(0, button.AbsolutePosition.X + (button.AbsoluteSize.X / 2), 0, button.AbsolutePosition.Y)
	end)
	button.MouseLeave:Connect(function()
		infoEl.Visible = false
	end)
end

local function makeSlider(slider, min, max, axis, default, resetButton, label, text, event)
	local result
	local filling = slider:FindFirstChild("Filling")
	local Button = Instance.new("TextButton")
	local dragging = false
	Button.Name = "Button"
	Button.Parent = slider
	Button.AnchorPoint = Vector2.new(0.5, 0.5)
	Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Button.BackgroundTransparency = 1.000
	Button.Position = UDim2.new(0.5, 0, 0.5, 0)
	Button.Size = UDim2.new(1, 0, 1, 0)
	Button.ZIndex = 10
	Button.Font = Enum.Font.SourceSans
	Button.Text = ""
	Button.TextColor3 = Color3.fromRGB(0, 0, 0)
	Button.TextSize = 14.000

	if resetButton then
		local Button2 = Instance.new("TextButton")
		Button2.Name = "Button"
		Button2.Parent = resetButton
		Button2.AnchorPoint = Vector2.new(0.5, 0.5)
		Button2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Button2.BackgroundTransparency = 1.000
		Button2.Position = UDim2.new(0.5, 0, 0.5, 0)
		Button2.Size = UDim2.new(1, 0, 1, 0)
		Button2.ZIndex = 10
		Button2.Font = Enum.Font.SourceSans
		Button2.Text = ""
		Button2.TextColor3 = Color3.fromRGB(0, 0, 0)
		Button2.TextSize = 14.000
	end

	Button.MouseButton1Down:Connect(function()
		dragging = true
	end)
	uis.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			if dragging then dragging = false end
		end
	end)
	uis.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			if dragging then
				local mouseLoc = uis:GetMouseLocation()
				local resultNum
				if axis == "X" then
					local relativePos = mouseLoc - slider.AbsolutePosition
					local percentage = math.clamp(relativePos.X / slider.AbsoluteSize.X, 0, 1)
					filling.Visible = false
					if percentage ~= 0 then filling.Visible = true end
					filling.Size = UDim2.new(percentage, 0, 1, 0)
					resultNum = percentage * max
				else
					local relativePos = mouseLoc - (slider.AbsolutePosition + slider.AbsoluteSize / 3.5)
					local percentage = math.clamp(relativePos.Y / slider.AbsoluteSize.Y, 0, 1)
					filling.Visible = false
					if percentage ~= 1 then filling.Visible = true end
					filling.Size = UDim2.new(1, 0, 1 - percentage, 0)
					resultNum = 1 - percentage * max
				end
				if resultNum < min then resultNum = min end
				if label then
					label.Text = math.round(resultNum).." "..text
				end
				if event then result = resultNum end
				event(result)
			end
		end
	end)
	if resetButton then
		resetButton.Button.MouseButton1Click:Connect(function()
			result = default
			label.Text = math.round(result).." "..text
			if default == 0 then
				filling.Visible = false
			end
			if axis == "X" then
				filling.Size = UDim2.new(0, 0, 1, 0)
			else
				filling.Size = UDim2.new(1, 0, 0, 0)
			end
			event(result)
		end)
	end
	if not filling.Visible then filling.Visible = true end
end
