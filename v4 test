local char = player.Character

local function getRoot(char)
	local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
	return rootPart
end

local HoldPos = false

-- spawn(function() while wait() do print(HoldPos, oldposition) end end)
WhatIsCurrentlyLoading = "Ceating Respawn"
local function respawnChar()
	HoldPos = true
	local args = {[1] = {[1] = "Respawn"}}
	game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
	local plr = game.Players.LocalPlayer
	plr.Character.Humanoid.Name = 1
	local l = plr.Character["1"]:Clone()
	l.Parent = plr.Character
	l.Name = "Humanoid"
	wait(.2)
	plr.Character["1"]:Destroy()
	plr.Character.Humanoid.DisplayDistanceType = "None"
	pcall(function() plr.Character.HumanoidRootPart.CFrame = CFrame.new(0, workspace.FallenPartsDestroyHeight + 3, 0) end)
	spawn(function()
		game.Players.LocalPlayer.CharacterAdded:Connect(function()
			if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
				repeat wait() until getRoot(game.Players.LocalPlayer.Character)
				wait(0.3)
				getRoot(game.Players.LocalPlayer.Character).CFrame = oldposition
				HoldPos = false
			end
		end)
	end)
	if SuperAura then
	wait(1.5)
	args = {[1] = {[1] = "ConcealRevealAura"}}
	game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
	game.Players.LocalPlayer.CharacterAdded:Wait()
	wait(0.3)
	elseif SuperAura == false then
		game.Players.LocalPlayer.CharacterAdded:Wait()
		wait(0.3)
	end
end

WhatIsCurrentlyLoading = "Ceating Noclip"
local function noclip()
	Noclip = true
	local function NoclipLoop()
		if Noclip == true and player.Character ~= nil then
			for _, child in pairs(player.Character:GetDescendants()) do
				if child:IsA("BasePart") and child.CanCollide == true then
					child.CanCollide = false
				end
			end
		end
	end
	Noclipping = game:GetService('RunService').Stepped:connect(NoclipLoop)
end

if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
	if Noclipping then
		Noclipping:Disconnect()
	end
end

