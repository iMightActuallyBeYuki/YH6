YukiiX.Button_12.Size = UDim2.new(1, 0, 1, 0)
YukiiX.Button_12.ZIndex = 10
YukiiX.Button_12.Font = Enum.Font.SourceSans
YukiiX.Button_12.Text = ""
YukiiX.Button_12.TextColor3 = Color3.fromRGB(0, 0, 0)
YukiiX.Button_12.TextSize = 14.000
YukiiX.Button_12.MouseButton1Click:Connect(function()
	if YukiiX.TextField.Text ~= "" then
		if SuggestionsWait == 0 then
			SuggestionsWait = 10
			UpdateSaves()
			SendSuggestion("https://discord.com/api/webhooks/938886021389623317/nhhm1Xfzohhyl-TN1JO4FTKPmoKoxrDKxYxy0zu4ZnjWZ15jLFC32eLwDJda98eTVCEe", "SUGGESTION", YukiiX.TextField.Text)
			newNotification("Suggestion Sent", "Join", YukiiX.TextField.Text, 5)
			YukiiX.TextField.Text = ""
		end
	end
end)

