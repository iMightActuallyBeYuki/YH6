WhatIsCurrentlyLoading = "Ceating Friends List"
local friendPlace = {}
local function createFriendInfo()
	table.clear(friendPlace)
	local friendslist = game:HttpGet("https://friends.roblox.com/v1/users/"..game.Players.LocalPlayer.UserId.."/friends?userSort=Alphabetical")
	local friendscount = game:HttpGet("https://friends.roblox.com/v1/users/"..game.Players.LocalPlayer.UserId.."/friends/count")
	
	local newfriendlist = game:GetService('HttpService'):JSONDecode(friendslist)
	
	local function getPresence(presence)
		if presence == 0 then
			return "Offline"
		elseif presence == 1 then
			return "Online"
		elseif presence == 2 then
			return "Playing"
		elseif presence == 3 then
			return "Studio"
		end
	end
	
	local online = 0
	local offline = 0
	local all = string.gsub(string.gsub(friendscount, "{\"count\":", ""), "}", "")
	local server = 0
	
	local onlineFriends = game.Players.LocalPlayer:GetFriendsOnline()
	local homeHud = yx:WaitForChild("HomeHud")
	for i, data in pairs(newfriendlist.data) do
		local name = data.name
		local id = data.id
		local displayName = data.displayName
		local presenceType = data.presenceType

		if getPresence(presenceType) ~= "Offline" then
			local profile = assets.UserElement:Clone()
			profile.Parent = homeHud.Friends.PlayerList.ScrollingFrame
			local color = profile[getPresence(presenceType).."Icon"].ImageColor3
			profile.Name = name
			profile.UserName.Text = "@"..name
			profile.DisplayName.Text = displayName
			profile.PlayingIcon.Visible = false
			profile.UserIcon.Image = Players:GetUserThumbnailAsync(id, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
			profile[getPresence(presenceType).."Icon"].Visible = true
			profile.ActionLabel.Text = "Player has joins disabled"
			profile.ActionLabel.TextColor3 = color

			profile.JoinButtonFrame.Visible = false
			if getPresence(presenceType) == "Playing" then
				profile.JoinButtonFrame.Visible = true
				profile.JoinButtonFrame.Button.MouseButton1Click:Connect(function()
					for i, v in pairs(friendPlace) do
						if v[1] == id then
							tpService:TeleportToPlaceInstance(v[2], v[3], player)
						end
					end
				end)
			elseif getPresence(presenceType) == "Online" then
				profile.ActionLabel.TextColor3 = color
				profile.ActionLabel.Text = "Online"
			end
		
			for i, friend in pairs(onlineFriends) do
				if friend.VisitorId == id then
					if friend.PlaceId ~= nil then
						if getPresence(presenceType) == "Playing" then
							profile.ActionLabel.Text = "Playing: "..game:GetService("MarketplaceService"):GetProductInfo(friend.PlaceId).Name
						elseif getPresence(presenceType) == "Studio" then
							profile.ActionLabel.Text = "Editing: "..game:GetService("MarketplaceService"):GetProductInfo(friend.PlaceId).Name
						end
						table.insert(friendPlace, {friend.VisitorId, friend.PlaceId, friend.GameId})
						break
					end
				end
			end
			
			for i,v in pairs(game.Players:GetPlayers()) do
				if v.Name == name then
					server += 1
				end
			end
			if getPresence(presenceType) == "Online" or getPresence(presenceType) == "Playing" or getPresence(presenceType) == "Studio" then online += 1 end
			offline = all - online
			homeHud.Friends.Friends.All.Count.Text = all
			homeHud.Friends.Friends.Online.Count.Text = online
			homeHud.Friends.Friends.Offline.Count.Text = offline
			homeHud.Friends.Friends.Server.Count.Text = server
		end
	end
end

createFriendInfo()

local function updateFriendInfo()
	table.clear(friendPlace)
	local friendslist = game:HttpGet("https://friends.roblox.com/v1/users/"..game.Players.LocalPlayer.UserId.."/friends?userSort=Alphabetical")
	local friendscount = game:HttpGet("https://friends.roblox.com/v1/users/"..game.Players.LocalPlayer.UserId.."/friends/count")
	
	local newfriendlist = game:GetService('HttpService'):JSONDecode(friendslist)
	
	local function getPresence(presence)
		if presence == 0 then
			return "Offline"
		elseif presence == 1 then
			return "Online"
		elseif presence == 2 then
			return "Playing"
		elseif presence == 3 then
			return "Studio"
		end
	end
	
	local online = 0
	local offline = 0
	local all = string.gsub(string.gsub(friendscount, "{\"count\":", ""), "}", "")
	local server = 0
	
	local onlineFriends = game.Players.LocalPlayer:GetFriendsOnline()
	local homeHud = yx:WaitForChild("HomeHud")
	for i, data in pairs(newfriendlist.data) do
		local name = data.name
		local id = data.id
		local displayName = data.displayName
		local presenceType = data.presenceType

		local hasProfile

		if getPresence(presenceType) ~= "Offline" then
			local profile
			if homeHud.Friends.PlayerList.ScrollingFrame:FindFirstChild(name) then
				profile = homeHud.Friends.PlayerList.ScrollingFrame:FindFirstChild(name)
				hasProfile = true
			else
				profile = assets.UserElement:Clone()
				profile.Name = name
				profile.Parent = homeHud.Friends.PlayerList.ScrollingFrame
				hasProfile = false
			end
			local color = profile[getPresence(presenceType).."Icon"].ImageColor3
			profile.UserName.Text = "@"..name
			profile.DisplayName.Text = displayName
			profile.PlayingIcon.Visible = false
			profile.OfflineIcon.Visible = false
			profile.StudioIcon.Visible = false
			profile.OnlineIcon.Visible = false
			profile.UserIcon.Image = Players:GetUserThumbnailAsync(id, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
			profile[getPresence(presenceType).."Icon"].Visible = true
			profile.ActionLabel.TextColor3 = color

			profile.JoinButtonFrame.Visible = false
			if getPresence(presenceType) == "Playing" then
				profile.JoinButtonFrame.Visible = true
				if not hasProfile then
					profile.JoinButtonFrame.Visible = true
					profile.JoinButtonFrame.Button.MouseButton1Click:Connect(function()
						for i, v in pairs(friendPlace) do
							if v[1] == id then
								tpService:TeleportToPlaceInstance(v[2], v[3], player)
							end
						end
					end)
				end
			elseif getPresence(presenceType) == "Online" then
				profile.ActionLabel.TextColor3 = color
				profile.ActionLabel.Text = "Online"
			end
		
			for i, friend in pairs(onlineFriends) do
				if friend.VisitorId == id then
					if friend.PlaceId ~= nil then
						if getPresence(presenceType) == "Playing" then
							profile.ActionLabel.Text = "Player has joins disabled"
							profile.ActionLabel.Text = "Playing: "..game:GetService("MarketplaceService"):GetProductInfo(friend.PlaceId).Name
						elseif getPresence(presenceType) == "Studio" then
							profile.ActionLabel.Text = "Editing: "..game:GetService("MarketplaceService"):GetProductInfo(friend.PlaceId).Name
						end
						table.insert(friendPlace, {friend.VisitorId, friend.PlaceId, friend.GameId})
						break
					end
				end
			end
			
			for i,v in pairs(game.Players:GetPlayers()) do
				if v.Name == name then
					server += 1
				end
			end
			if getPresence(presenceType) == "Online" or getPresence(presenceType) == "Playing" or getPresence(presenceType) == "Studio" then online += 1 end
			offline = all - online
			homeHud.Friends.Friends.All.Count.Text = all
			homeHud.Friends.Friends.Online.Count.Text = online
			homeHud.Friends.Friends.Offline.Count.Text = offline
			homeHud.Friends.Friends.Server.Count.Text = server
		else
			local profile = homeHud.Friends.PlayerList.ScrollingFrame:FindFirstChild(name)
			if profile then profile:Destroy() end
		end
	end
end

spawn(function()
	while wait(60) do
		if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then break end
		updateFriendInfo()
	end
end)

local controllerButtons = yx.MainController.ControllerButtons.ControllerButtonHolder

for i, object in pairs(controllerButtons:GetChildren()) do
	if object:IsA("Frame") then
		object.Button.MouseButton1Click:Connect(function()
			for i, frame in pairs(controllerButtons:GetChildren()) do
				if frame:IsA("Frame") then
					tweenControllerButton(frame, false)
				end
			end
			if object.BackgroundColor3 == Color3.fromRGB(255, 255, 255) or object.BackgroundColor3 == Color3.fromRGB(255, 145, 90) then
				tweenControllerButton(object, false)

				tweenOthers(nil)
				visibleHud = nil
			else
				tweenControllerButton(object, true)
				local objectName = string.gsub(object.Name, "Frame", "")

				tweenOthers(objectName)
				tweenHud(objectName, true)
				visibleHud = objectName
			end
		end)
	end
end

yx.MainController.CloseBackground.CloseFrame.Button.MouseButton1Click:Connect(function()
	transparencyTween(yx.MainController, 0.2, mainControllerData, false)
	transparencyTween(yx.ToggleButton, 0.2, toggleButtonData, true)
	tweenOthers(nil)
end)

yx.ToggleButton.Button.MouseButton1Click:Connect(function()
	transparencyTween(yx.MainController, 0.2, mainControllerData, true)
	transparencyTween(yx.ToggleButton, 0.2, toggleButtonData, false)
	tweenHud(visibleHud, true)
end)

local uiDebounce = false

uis.InputBegan:Connect(function(input, isTyping)
	if isTyping then return end
	if changingKey then
		uiKey = input.KeyCode.Name
		OpenKeybind = uiKey
		yx.HomeHud.Keybind.KeyFrame.Button.Text = uiKey
		changingKey = false
		return
	end
	if input.KeyCode == Enum.KeyCode[uiKey] then
		if uiDebounce then return end
		uiDebounce = true
		delay(0.5, function()
			uiDebounce = false
		end)
		if yx:WaitForChild("ToggleButton").Visible then
			transparencyTween(yx.MainController, 0.2, mainControllerData, true)
			transparencyTween(yx.ToggleButton, 0.2, toggleButtonData, false)
			tweenHud(visibleHud, true)
		else
			transparencyTween(yx.MainController, 0.2, mainControllerData, false)
			transparencyTween(yx.ToggleButton, 0.2, toggleButtonData, true)
			tweenOthers(nil)
		end
	end
end)

yx.HomeHud.Keybind.KeyFrame.Button.MouseButton1Click:Connect(function()
	changingKey = true
	yx.HomeHud.Keybind.KeyFrame.Button.Text = "Press a Key"
end)

