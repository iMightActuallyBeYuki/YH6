WhatIsCurrentlyLoading = "Creating Suggestion/Bugs"
local webhookcheck = is_sirhurt_closure and "Sirhurt" or pebc_execute and "ProtoSmasher" or syn and "Synapse X" or
secure_load and "Sentinel" or
KRNL_LOADED and "Krnl" or
SONA_LOADED and "Sona" or
"An Unknown Executor"

local function SendSuggestion(url, title, text)
local data = {
["embeds"] = {
    {
        ["title"] = title,
        ["description"] = text.."\n\n**Username: "..game.Players.LocalPlayer.Name.."**",
        ["type"] = "rich",
        ["color"] = tonumber(0x7269da),
        ["image"] = {}
    }
}
}
local newdata = game:GetService("HttpService"):JSONEncode(data)

local headers = {
["content-type"] = "application/json"
}
request = http_request or request or HttpPost or syn.request
local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
request(abcdef)
end

local randomlygeneratednumber = math.random()
getgenv().SuggestionsWait = 0
getgenv().IsSuggestionsWaiting = randomlygeneratednumber
IsSuggestionsWaiting = randomlygeneratednumber
local ClientIsSuggestionsWaiting = IsSuggestionsWaiting

spawn(function()		
	if ClientIsSuggestionsWaiting ~= IsSuggestionsWaiting then coroutine.yield() end
while ClientIsSuggestionsWaiting == IsSuggestionsWaiting do
	if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
		wait(1)
		if ClientIsSuggestionsWaiting ~= IsSuggestionsWaiting then coroutine.yield() end
		if SuggestionsWait ~= 0 then
			if ClientIsSuggestionsWaiting ~= IsSuggestionsWaiting then coroutine.yield() end
		SuggestionsWait = SuggestionsWait - 1
		UpdateSaves()
		end
	end
end
end)

