TimeButtonHaha.AnchorPoint = Vector2.new(0.5, 0.5)
TimeButtonHaha.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TimeButtonHaha.BackgroundTransparency = 1.000
TimeButtonHaha.Position = UDim2.new(0.5, 0, 0.5, 0)
TimeButtonHaha.Size = UDim2.new(1, 0, 1, 0)
TimeButtonHaha.ZIndex = 10
TimeButtonHaha.Font = Enum.Font.SourceSans
TimeButtonHaha.Text = ""
TimeButtonHaha.TextColor3 = Color3.fromRGB(0, 0, 0)
TimeButtonHaha.TextSize = 14.000

TimeButtonHaha.MouseButton1Click:Connect(function()
	if TimeFormat == "military" then TimeFormat = "standard" else TimeFormat = "military" end
end)

spawn(function()
	WhatIsCurrentlyLoading = "Getting Time"
	while wait(0.1) do
		if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
			local TimeInUnix = os.time()
			local stringToFormat = "%H:%M"
			local result = os.date(stringToFormat, TimeInUnix)
			if TimeFormat == "military" then
				result = result
			else
				local curHour = tonumber(string.sub(result, 1, 2))
				if curHour > 12 then
					result = string.gsub(result, tostring(curHour), tostring(curHour - 12)).."pm"
				else
					result = result.."am"
				end
			end
			
			YukiiX.TimeLabel.Text = result
		end
	end
end)

