WhatIsCurrentlyLoading = "Ceating Player List"
_G.playerAttackActions = {}
local prevSelectedUser = nil
local curSelectedUser = nil
local pListCreated = false
local PlayerButtonScale = 0
local StatsFrameScale = 0
local AttackFrameScale = 0

local function actionsMenu(plr)
	local plrStats = plr.PrivateStats
	local sActionList = yx.PeopleHud.UserStatsFrame.ActionFrame.ActionList
	local actionList = yx.PeopleHud.UserActionFrame.ActionFrame.ActionList
	if StatsFrameScale == 0 then StatsFrameScale = sActionList.Fist.AbsoluteSize.Y end
	if AttackFrameScale == 0 then AttackFrameScale = actionList.Soul.AbsoluteSize.Y end
	if not yx.PeopleHud.UserStatsFrame.Visible then
		if curSelectedUser == plr then
			yx.PeopleHud.UserStatsFrame.Visible = false
			curSelectedUser = nil
		else
			yx.PeopleHud.UserStatsFrame.Visible = true
			curSelectedUser = plr
		end
	else
		if curSelectedUser == plr then
			yx.PeopleHud.UserStatsFrame.Visible = false
			curSelectedUser = nil
		else
			yx.PeopleHud.UserStatsFrame.Visible = true
			curSelectedUser = plr
		end
	end
	if prevSelectedUser then
		yx.PeopleHud.ActionFrame.Playerlist:FindFirstChild(prevSelectedUser.Name).BackgroundColor3 = Color3.fromRGB(44, 44, 44)
	end
	if curSelectedUser then
		prevSelectedUser = curSelectedUser
		yx.PeopleHud.ActionFrame.Playerlist:FindFirstChild(curSelectedUser.Name).BackgroundColor3 = Color3.fromRGB(100, 100, 100)
	end
	for i, v in pairs(sActionList:GetChildren()) do
		if v.Name ~= "shittop" and v.Name ~= "shitbottom" and v.Name ~= "UIListLayout" then
			v.Size = UDim2.new(v.Size.X.Scale, v.Size.X.Offset, 0, StatsFrameScale)
		end
	end
	for i, v in pairs(actionList:GetChildren()) do
		if v.Name ~= "shittop" and v.Name ~= "shitbottom" and v.Name ~= "UIListLayout" then
			v.Size = UDim2.new(v.Size.X.Scale, v.Size.X.Offset, 0, AttackFrameScale)
		end
	end
	sActionList.UIListLayout.Padding = UDim.new(0, 10)
	sActionList.CanvasSize = UDim2.new(0, 0, 0, sActionList.UIListLayout.AbsoluteContentSize.Y + 2.5)
	actionList.UIListLayout.Padding = UDim.new(0, 10)
	actionList.CanvasSize = UDim2.new(0, 0, 0, actionList.UIListLayout.AbsoluteContentSize.Y + 2.5)
	spawn(function()
		while wait(0.1) do
			if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then break end
			if not curSelectedUser then break end
			plrStats = curSelectedUser.PrivateStats
			sActionList.Fist.Label.Text = "Fist:\n"..converttoletter(plrStats.FistStrength.Value)
			sActionList.Body.Label.Text = "Body:\n"..converttoletter(plrStats.BodyToughness.Value)
			sActionList.Psychic.Label.Text = "Psychic:\n"..converttoletter(plrStats.PsychicPower.Value)
			sActionList.Speed.Label.Text = "Speed:\n"..converttoletter(plrStats.MovementSpeed.Value)
			sActionList.Jump.Label.Text = "Jump:\n"..converttoletter(plrStats.JumpForce.Value)
		end
	end)
	yx.PeopleHud.UserActionFrame.Visible = yx.PeopleHud.UserStatsFrame.Visible
end

local function attackOptions(plr)
	local sActionList = yx.PeopleHud.UserStatsFrame.ActionFrame.ActionList
	local actionList = yx.PeopleHud.UserActionFrame.ActionFrame.ActionList
	--// Soul Attack Plr
	actionList.Soul.Buttons.Soul.Button.MouseButton1Click:Connect(function()
		if _G.playerAttackActions[plr.Name].SoulAttack == true then
			_G.playerAttackActions[plr.Name].SoulAttack = false
			repStor.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
			actionList.Soul.Buttons.Soul.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
		else
			_G.playerAttackActions[plr.Name].SoulAttack = true
			actionList.Soul.Buttons.Soul.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
			spawn(function()
				while _G.playerAttackActions[plr.Name].SoulAttack == true do
					if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
					repStor.RemoteEvent:FireServer({"Skill_SoulAttack_Start", plr})
					repStor.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
					wait(0.1)
				end
			end)
		end
	end)
	if _G.playerAttackActions[plr.Name].SoulAttack then
		spawn(function()
			while _G.playerAttackActions[plr.Name].SoulAttack == true do
				if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
				repStor.RemoteEvent:FireServer({"Skill_SoulAttack_Start", curSelectedUser})
				repStor.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
				wait(0.1)
			end
		end)
		actionList.Soul.Buttons.Soul.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
	end

	--// Soul Attack All
	actionList.Soul.Buttons.SoulA.Button.MouseButton1Click:Connect(function()
		if _G.playerAttackActions[plr.Name].SoulAttackAll == true then
			_G.playerAttackActions[plr.Name].SoulAttackAll = false
			repStor.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
			actionList.Soul.Buttons.SoulA.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
		else
			_G.playerAttackActions[plr.Name].SoulAttackAll = true
			repStor.RemoteEvent:FireServer({"Skill_SoulAttack_Start", curSelectedUser})
			actionList.Soul.Buttons.SoulA.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
			spawn(function()
				while _G.playerAttackActions[plr.Name].SoulAttackAll == true do
					if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
					for i, v in pairs(Players:GetPlayers()) do
						if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
						repStor.RemoteEvent:FireServer({"Skill_SoulAttack_Start", v})
						repStor.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
						wait(0.1)
					end
				end
			end)
		end
	end)
	if _G.playerAttackActions[plr.Name].SoulAttackAll then
		spawn(function()
			while _G.playerAttackActions[plr.Name].SoulAttackAll == true do
				if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
				for i, v in pairs(Players:GetPlayers()) do
					if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
					repStor.RemoteEvent:FireServer({"Skill_SoulAttack_Start", v})
					repStor.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
				end
			end
		end)
		actionList.Soul.Buttons.SoulA.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
	end

	--// Reap Plr
	actionList.Reap.Buttons.Reap.Button.MouseButton1Click:Connect(function()
		repStor.RemoteEvent:FireServer({"Skill_SoulReap", curSelectedUser})
	end)

	--// Loop Reap Plr
	actionList.Reap.Buttons.LReap.Button.MouseButton1Click:Connect(function()
		if _G.playerAttackActions[plr.Name].LoopReap == true then
			_G.playerAttackActions[plr.Name].LoopReap = false
			actionList.Reap.Buttons.LReap.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
		else
			_G.playerAttackActions[plr.Name].LoopReap = true
			actionList.Reap.Buttons.LReap.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
			spawn(function()
				while _G.playerAttackActions[plr.Name].LoopReap == true do
					if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
					repStor.RemoteEvent:FireServer({"Skill_SoulReap", curSelectedUser})
					wait(0.1)
				end
			end)
		end
	end)
	if _G.playerAttackActions[plr.Name].LoopReap then
		spawn(function()
			while _G.playerAttackActions[plr.Name].LoopReap == true do
				if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
				repStor.RemoteEvent:FireServer({"Skill_SoulReap", curSelectedUser})
				wait(0.1)
			end
		end)
		actionList.Reap.Buttons.LReap.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
	end

	--// Reap All
	actionList.Reap.Buttons.ReapA.Button.MouseButton1Click:Connect(function()
		if _G.playerAttackActions[plr.Name].ReapAll == true then
			_G.playerAttackActions[plr.Name].ReapAll = false
			actionList.Reap.Buttons.ReapA.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
		else
			_G.playerAttackActions[plr.Name].ReapAll = true
			actionList.Reap.Buttons.ReapA.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
			spawn(function()
				while _G.playerAttackActions[plr.Name].ReapAll == true do
					if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
					for i, v in pairs(Players:GetPlayers()) do
						if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
						if not v.Character:FindFirstChild("SafeZoneShield") then
						repStor.RemoteEvent:FireServer({"Skill_SoulReap", v})
						wait(0.25)
						end
					end
				end
			end)
		end
	end)
	if _G.playerAttackActions[plr.Name].ReapAll then
		spawn(function()
			while _G.playerAttackActions[plr.Name].ReapAll == true do
				if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
				for i, v in pairs(Players:GetPlayers()) do
					if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
					repStor.RemoteEvent:FireServer({"Skill_SoulReap", v})
					wait(0.25)
				end
			end
		end)
		actionList.Reap.Buttons.ReapA.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
	end
end

local function createPlayerList()
	for i, plr in pairs(Players:GetPlayers()) do
		if not _G.playerAttackActions[plr.Name] then
			table.insert(_G.playerAttackActions, plr.Name)
			local actions = {
				["SoulAttack"] = false,
				["SoulAttackAll"] = false,
				["LoopReap"] = false,
				["ReapAll"] = false,
				["LoopVoid"] = false,
				["LoopHellFire"] = false
			}
			_G.playerAttackActions[plr.Name] = actions
		end
		if plr == player then continue end
		local ui = assets.AttackFrame:Clone()
		ui.Name = plr.Name
		ui.Parent = yx.PeopleHud.ActionFrame.Playerlist
		local sActionList = yx.PeopleHud.UserStatsFrame.ActionFrame.ActionList
		local actionList = yx.PeopleHud.UserActionFrame.ActionFrame.ActionList
		if PlayerButtonScale == 0 then PlayerButtonScale = ui.AbsoluteSize.Y end
		if StatsFrameScale == 0 then StatsFrameScale = sActionList.Fist.AbsoluteSize.Y end
		if AttackFrameScale == 0 then AttackFrameScale = actionList.Soul.AbsoluteSize.Y end
		ui.NameDisplay.DisplayName.Text = plr.DisplayName
		ui.NameDisplay.Username.Text = plr.Name
		ui.Size = UDim2.new(ui.Size.X.Scale, ui.Size.X.Offset, 0, PlayerButtonScale)
		ui.UserIcon.Image = Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
		yx.PeopleHud.ActionFrame.Playerlist.UIListLayout.Padding = UDim.new(0, 10)
		yx.PeopleHud.ActionFrame.Playerlist.CanvasSize = UDim2.new(0, 0, 0, yx.PeopleHud.ActionFrame.Playerlist.UIListLayout.AbsoluteContentSize.Y + 2.5)
		
		ui.UserButtons.EspFrame.Visible = false
		ui.UserButtons.TeleportFrame.LayoutOrder = -5
		ui.UserButtons.TeleportFrame.Button.MouseButton1Click:Connect(function()
			local char = player.Character or player.CharacterAdded:Wait()
			local hrp = char:WaitForChild("HumanoidRootPart")

			if workspace:FindFirstChild(plr.Name) then
				hrp.CFrame = plr.Character:WaitForChild("HumanoidRootPart").CFrame
			else
				newNotification("Error", "Leave", "Player "..plr.Name.." has not spawned their character in yet, try again later", 2.5)
			end
		end)
		hoverInfo(ui.UserButtons.TeleportFrame.Button, "Teleport")
		local StatsFrameScale = 0
		local AttackFrameScale = 0
		ui.UserButtons.ActionsFrame.Button.MouseButton1Click:Connect(function()
			actionsMenu(plr)
			attackOptions(plr)
		end)

		hoverInfo(ui.UserButtons.ActionsFrame.Button, "Actions")
	end
	pListCreated = true
end
createPlayerList()
Players.PlayerAdded:Connect(function(plr)
	if pListCreated then
		if not _G.playerAttackActions[plr.Name] then
			table.insert(_G.playerAttackActions, plr.Name)
			local actions = {
				["SoulAttack"] = false,
				["SoulAttackAll"] = false,
				["LoopReap"] = false,
				["ReapAll"] = false,
				["LoopVoid"] = false,
				["LoopHellFire"] = false
			}
			_G.playerAttackActions[plr.Name] = actions
		end
		local ui = assets:WaitForChild("AttackFrame"):Clone()
		ui.Name = plr.Name
		ui.Parent = yx.PeopleHud.ActionFrame.Playerlist
		ui.NameDisplay.DisplayName.Text = plr.DisplayName
		ui.NameDisplay.Username.Text = plr.Name
		ui.Size = UDim2.new(ui.Size.X.Scale, ui.Size.X.Offset, 0, PlayerButtonScale)
		ui.UserIcon.Image = Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
		yx.PeopleHud.ActionFrame.Playerlist.UIListLayout.Padding = UDim.new(0, 10)
		yx.PeopleHud.ActionFrame.Playerlist.CanvasSize = UDim2.new(0, 0, 0, yx.PeopleHud.ActionFrame.Playerlist.UIListLayout.AbsoluteContentSize.Y + 2.5)
		
		ui.UserButtons.EspFrame.Visible = false
		ui.UserButtons.TeleportFrame.LayoutOrder = -5
		ui.UserButtons.TeleportFrame.Button.MouseButton1Click:Connect(function()
			local char = player.Character or player.CharacterAdded:Wait()
			local hrp = char:WaitForChild("HumanoidRootPart")

			if workspace:FindFirstChild(plr.Name) then
				hrp.CFrame = plr.Character:WaitForChild("HumanoidRootPart").CFrame
			else
				newNotification("Error", "Leave", "Player "..plr.Name.." has not spawned their character in yet, try again later", 2.5)
			end
		end)
		hoverInfo(ui.UserButtons.TeleportFrame.Button, "Teleport")
		ui.UserButtons.ActionsFrame.Button.MouseButton1Click:Connect(function()
			actionsMenu(plr)
		end)

		attackOptions(plr)
		hoverInfo(ui.UserButtons.ActionsFrame.Button, "Actions")
	end
end)

Players.PlayerRemoving:Connect(function(plr)
	if pListCreated then
		local ui = yx:WaitForChild("PeopleHud").ActionFrame.Playerlist:FindFirstChild(plr.Name)
		if ui then ui:Destroy() end
		if plr == curSelectedUser then curSelectedUser = nil end
		local sActionList = yx.PeopleHud.UserStatsFrame.ActionFrame.ActionList
		local actionList = yx.PeopleHud.UserActionFrame.ActionFrame.ActionList
		yx.PeopleHud.UserStatsFrame.Visible = false
		yx.PeopleHud.UserActionFrame.Visible = false
		yx.PeopleHud.ActionFrame.Playerlist.UIListLayout.Padding = UDim.new(0, 10)
		yx.PeopleHud.ActionFrame.Playerlist.CanvasSize = UDim2.new(0, 0, 0, yx.PeopleHud.ActionFrame.Playerlist.UIListLayout.AbsoluteContentSize.Y + 2.5)
		sActionList.UIListLayout.Padding = UDim.new(0, 10)
		sActionList.CanvasSize = UDim2.new(0, 0, 0, sActionList.UIListLayout.AbsoluteContentSize.Y + 2.5)
		actionList.UIListLayout.Padding = UDim.new(0, 10)
		actionList.CanvasSize = UDim2.new(0, 0, 0, actionList.UIListLayout.AbsoluteContentSize.Y + 2.5)
	end
end)

-- search bar:
local lastSearch = ""
spawn(function()
	while wait(0.1) do
		if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber)then break end
		for i, child in pairs(yx:WaitForChild("PeopleHud").ActionFrame.Playerlist:GetChildren()) do
			if not child:IsA("Frame") then continue end
			if child.Name == "shit_top" or child.Name == "shit_bottom" then continue end
			if string.match(string.lower(child.Name), string.lower(peopleSearchBar.Text)) then
				child.Visible = true
			elseif string.match(string.lower(Players[child.Name].DisplayName), string.lower(peopleSearchBar.Text)) then
				child.Visible = true
			else
				child.Visible = false
			end
			if lastSearch ~= peopleSearchBar.Text then
				yx.PeopleHud.ActionFrame.Playerlist.CanvasPosition = Vector2.new(0,0)
			end
			lastSearch = peopleSearchBar.Text
		end
	end
end)


WhatIsCurrentlyLoading = "Ceating Search Bar"
