local columns = yx.PlayerHud.ActionFrame.ActionButtonFrame
hoverInfo(columns.Column_1.NoclipFrame, "No Clip")
hoverInfo(columns.Column_1.NovoidFrame, "No Void")
hoverInfo(columns.Column_1.RefreshFrame, "Respawn")
