WhatIsCurrentlyLoading = "Ceating On Death Return"
local env = getsenv(game.Players.LocalPlayer.PlayerScripts.MainScript)
_G.loopCamera = true

local MenuData = {
MenuCamCFrame = CFrame.new(705.671021, 941.897827, 864.893188, 0.459262937, -0.712803423, 0.530083895, -0, 0.596739471, 0.8024351, -0.888300419, -0.368528694, 0.274060309),
MenuCamFocuss = CFrame.new(695.069336, 925.849121, 859.411987, 1, 0, 0, 0, 1, 0, 0, 0, 1)
}

spawn(function()
while _G.loopCamera do
	if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
    env.MenuCamCFrame = MenuData.MenuCamCFrame
    env.MenuCamFocuss = MenuData.MenuCamFocuss
	end
	wait()
end
end)

spawn(function()
	while wait(0.5) do
		if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
			if player.Character:FindFirstChild("HumanoidRootPart") and HoldPos == false then
				oldposition = player.Character.HumanoidRootPart.CFrame
			end
			if player.Character:FindFirstChild("Humanoid") then
				if player.Character.Humanoid.Health <= player.Character.Humanoid.Health /20 and ReturnOnDeath == true then
					game.ReplicatedStorage.RemoteEvent:FireServer({"Respawn"})
					spawn(function()
						while wait() do
							if ReturnOnDeath == true and game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
					game.Workspace.Camera.CameraType = "Custom"
							end
					end
					end)
					spawn(function()
						game.Players.LocalPlayer.CharacterAdded:Connect(function()
							if game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then
							repeat wait() until getRoot(game.Players.LocalPlayer.Character)
							wait(0.3)
							getRoot(game.Players.LocalPlayer.Character).CFrame = oldposition
							end
						end)
					end)
					if SuperAura == true then
						wait(1.5)
						args = {[1] = {[1] = "ConcealRevealAura"}}
						game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
					end
						wait(1.5)
							if ReturnOnDeath == true then
								pGui.IntroGui.Enabled = false
								pGui.ScreenGui.Enabled = true
								game:GetService("Lighting").Blur.Enabled = false
							elseif ReturnOnDeath == false then
								pGui.IntroGui.Enabled = true
								pGui.ScreenGui.Enabled = false
								game:GetService("Lighting").Blur.Enabled = true
						end
					wait()
				end
				wait()
			end
		end
	end
	end)

if ReturnOnDeath == true then
	tweenColor(columns.Column_2.ReturnFrame, Color3.fromRGB(140, 255, 140), 0.1)
	MenuData.MenuCamCFrame = 0
	MenuData.MenuCamFocuss = 0
end

