spawn(function()
	while wait(0.1) do
		if not game.CoreGui:FindFirstChild("Yukii X" .. KeepRandomNumber) then break end
		for i,v in pairs(game.CoreGui:GetChildren()) do
			if string.find(v.Name, "ESP_") then
				local plr = game.Players:FindFirstChild(string.gsub(v.Name, "ESP_", ""))
				repeat wait() until plr.Character
				repeat wait() until plr.Character:FindFirstChild("Head")
				v.Adornee = game.Players:FindFirstChild(string.gsub(v.Name, "ESP_", "")).Character.Head
			end
		end
	end
end)

newScript("Dark Menu", "Enable Dark Mode for the Menu", Color3.fromRGB(150, 165, 255), false, function(value, exe, isCheck, isForece)
	if not exe and not isCheck then DarkModeAutoExe = value end
	if isCheck and not exe then return DarkModeAutoExe end
	if exe and not isCheck then
		if not _G.darkMenu or isForce then
			WhatIsCurrentlyLoading = "Loading Dark Menu"
			_G.darkMenu = true
			loadstring(game:HttpGet("https://raw.githubusercontent.com/1ArcX/Scripts/main/Dark%20Menu", true))()
		end
	end
end)

