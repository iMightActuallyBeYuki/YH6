		game:GetService("UserInputService").InputBegan:Connect(function(input, isTyping)
			if input.KeyCode == Enum.KeyCode.LeftAlt and not isTyping then
				YukiiX.Enabled = not YukiiX.Enabled
			end
		end)

		if Enabled == true then
			Background:TweenSizeAndPosition(UDim2.new(0.955919802, 0, 0.866044998, 0), UDim2.new(0.0230590086, 0, 0.107542381, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, false)
			Main.Visible = true
			Player.Visible = true
			Misc.Visible = true
			About.Visible = true
			Scripts.Visible = true
			CloseButton.Rotation = 0
		elseif Enabled == false then
			Background:TweenSizeAndPosition(UDim2.new(0.956, 0, 0.555, 0), UDim2.new(0.023, 0, 0.418, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, false)
			CloseButton.Rotation = 180
			wait(0.5)
			Main.Visible = false
			Player.Visible = false
			Misc.Visible = false
			About.Visible = false
			Scripts.Visible = false
		end

		function Hide()
			if Enabled == true then
				Background:TweenSizeAndPosition(UDim2.new(0.955919802, 0, 0.866044998, 0), UDim2.new(0.0230590086, 0, 0.107542381, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, false)
				Main.Visible = true
				Player.Visible = true
				Misc.Visible = true
				About.Visible = true
				Scripts.Visible = true
				CloseButton.Rotation = 0
			elseif Enabled == false then
				Background:TweenSizeAndPosition(UDim2.new(0.956, 0, 0.555, 0), UDim2.new(0.023, 0, 0.418, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.5, false)
				CloseButton.Rotation = 180
				wait(0.5)
				Main.Visible = false
				Player.Visible = false
				Misc.Visible = false
				About.Visible = false
				Scripts.Visible = false
			end
			UpdateSaves()
		end


		CloseButton.MouseButton1Click:Connect(function()
			Enabled = not Enabled
			Hide()
		end)
	end

	while wait(1) do
		YukiiXLoadingScreen.Name = math.random(1,100000)
	end
end)
