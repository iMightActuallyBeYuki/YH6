		Psychic.Name = "Psychic"
		Psychic.Parent = P1
		Psychic.BackgroundColor3 = Color3.fromRGB(68, 68, 68)
		Psychic.BorderColor3 = Color3.fromRGB(55, 55, 55)
		Psychic.BorderSizePixel = 0
		Psychic.Position = UDim2.new(0.0428313538, 0, 0.0432492904, 0)
		Psychic.Size = UDim2.new(0.257314742, 0, 0.253317267, 0)
		Psychic.Font = Enum.Font.SourceSansSemibold
		Psychic.Text = "Psychic"
		Psychic.TextColor3 = Color3.fromRGB(255, 255, 255)
		Psychic.TextScaled = true
		Psychic.TextSize = 14.000
		Psychic.TextWrapped = true

		local SA = false
		local SAA = false

		if P == 1 then
			Psychic.Text = "Soul Attack (OFF)"
		end

		if P == 3 then
			Psychic.Text = "Soul Attack All (OFF)"
		end

		function SoulAttack(Random)
			spawn(function()
				game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_Start",Random})
				game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
			end)
		end

		Psychic.MouseButton1Click:Connect(function()
			if PlayerChosen then
			if P == 1 then
				SA = not SA
				while SA == true do
					Psychic.Text = "Soul Attack (ON)"
					Psychic.BackgroundColor3 = Color3.fromRGB(0,255,0)
					for i,v in pairs(game.Players:GetPlayers()) do
						pcall(function()
					game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_Start",PlayerChosen})
					game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
					end)
					wait(0)
					end
				end
				if SA == false then
					Psychic.BackgroundColor3 = Color3.fromRGB(68, 68, 68)
					Psychic.Text = "Soul Attack (OFF)"
				end
			end
		end
			if P == 3 then
				SAA = not SAA
				while SAA == true do
					Psychic.BackgroundColor3 = Color3.fromRGB(0,255,0)
					Psychic.Text = "Soul Attack All (ON)"
					wait(0.001)
					for i,v in pairs(game.Players:GetPlayers()) do
						local Random = game.Players:GetPlayers()[math.random(1,#game.Players:GetPlayers())]
						if Random.PrivateStats.PsychicPower.Value <= game:GetService("Players").LocalPlayer.PrivateStats.PsychicPower.Value/10 and not table.find(Whitelisted, Random.Name) then
						   SoulAttack(Random);
				end
				end
					wait(0)
				end
				if SA == false then
					Psychic.BackgroundColor3 = Color3.fromRGB(68, 68, 68)
					Psychic.Text = "Soul Attack All (OFF)"
				end
			end
		end)

		Psychic.MouseButton2Down:connect(
			function()
				PsychicInfo.Visible = true
			end
		)

		PsychicInfo.MouseEnter:connect(
			function()
				PsychicInfo.Visible = true
			end
		)

		PsychicInfo.MouseLeave:connect(
			function()
				PsychicInfo.Visible = false
			end
		)

		UICorner_2.CornerRadius = UDim.new(0, 10)
		UICorner_2.Parent = Psychic

		SpherePunch.Name = "Sphere Punch"
		SpherePunch.Parent = P1
		SpherePunch.BackgroundColor3 = Color3.fromRGB(68, 68, 68)
		SpherePunch.BorderColor3 = Color3.fromRGB(55, 55, 55)
		SpherePunch.BorderSizePixel = 0
		SpherePunch.Position = UDim2.new(0.0428313538, 0, 0.358351231, 0)
		SpherePunch.Size = UDim2.new(0.257314742, 0, 0.253317267, 0)
		SpherePunch.Font = Enum.Font.SourceSansSemibold
		SpherePunch.Text = "Sphere Punch"
		SpherePunch.TextColor3 = Color3.fromRGB(255, 255, 255)
		SpherePunch.TextScaled = true
		SpherePunch.TextSize = 14.000
		SpherePunch.TextWrapped = true

		local LSPT = false

		if SP == 1 then
			SpherePunch.Text = "Sphere Punch"
		end

		if SP == 2 then
			SpherePunch.Text = "Loop Sphere Punch (OFF)"
		end

		SpherePunch.MouseButton1Click:Connect(function()
			if PlayerChosen then
			if SP == 1 then
					local args = {
						[1] = {
							[1] = "Skill_SpherePunch",
							[2] = PlayerChosen.Character.HumanoidRootPart.Position
						}
					}
					game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
					wait()
				end
			if SP == 2 then
				LSPT = not LSPT
				while LSPT == true do
					SpherePunch.BackgroundColor3 = Color3.fromRGB(0,255,0)
					SpherePunch.Text = "Loop Sphere Punch (ON)"
					local args = {
						[1] = {
							[1] = "Skill_SpherePunch",
							[2] = PlayerChosen.Character.HumanoidRootPart.Position
						}
					}
					game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
					wait()
				end
				if SA == false then
					SpherePunch.BackgroundColor3 = Color3.fromRGB(68, 68, 68)
					SpherePunch.Text = "Loop Sphere Punch (OFF)"
				end
			end
			end
		end)

		SpherePunch.MouseButton2Click:connect(
			function()
				SpherePunchInfo.Visible = true
			end
		)

		SpherePunchInfo.MouseEnter:connect(
			function()
				SpherePunchInfo.Visible = true
			end
		)

		SpherePunchInfo.MouseLeave:connect(
			function()
				SpherePunchInfo.Visible = false
			end
		)

