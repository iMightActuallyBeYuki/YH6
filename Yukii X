local Enabled = true
local SelectedTab = 5
local Location = {X = 0.319444448, Y = 0.268729627}
function LoadDefaults()
Enabled = true
SelectedTab = 5
Location = {X = 0.319444448, Y = 0.268729627}
end

function UpdateSaves()
local Update = {
Enabled = Enabled;
SelectedTab = SelectedTab;
Location = Location;
}
writefile("X.lua",
game:GetService("HttpService"):JSONEncode(Update))
end

function LoadSaves()
local success, errorsend = pcall(function()
saves = game:GetService("HttpService"):JSONDecode(readfile("X.lua"))
end)
    if not success then
        LoadDefaults()
        UpdateSaves()
        return
    end
Enabled = saves.Enabled
SelectedTab = saves.SelectedTab
end
if writefile and readfile then
    LoadSaves()
else
    LoadDefaults()
end

