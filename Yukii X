_G.YUKIIXDRAG = {}

function createDrag(object)
_G.YUKIIXDRAG[object] = {}
object.MouseEnter:Connect(
	function()
		_G.YUKIIXDRAG[object].checkMouse = true
		_G.YUKIIXDRAG[object].mdwn =
			object.InputBegan:Connect(
			function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					_G.YUKIIXDRAG[object].startpos = object.Position
					_G.YUKIIXDRAG[object].startdrag = input.Position
					_G.YUKIIXDRAG[object].mouseDown = true
					_G.YUKIIXDRAG[object].mou = input
					_G.YUKIIXDRAG[object].mloo =
						game:GetService("RunService").RenderStepped:Connect(
						function()
							if _G.YUKIIXDRAG[object].mouseDown then
								_G.YUKIIXDRAG[object].delta =
									_G.YUKIIXDRAG[object].mou.Position - _G.YUKIIXDRAG[object].startdrag
								object.Position =
									UDim2.new(
									_G.YUKIIXDRAG[object].startpos.X.Scale,
									_G.YUKIIXDRAG[object].startpos.X.Offset + _G.YUKIIXDRAG[object].delta.X,
									_G.YUKIIXDRAG[object].startpos.Y.Scale,
									_G.YUKIIXDRAG[object].startpos.Y.Offset + _G.YUKIIXDRAG[object].delta.Y
								)
							end
						end
					)
				end
			end
		)
		_G.YUKIIXDRAG[object].mmoved =
			object.InputChanged:Connect(
			function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement then
					_G.YUKIIXDRAG[object].mou = input
				end
			end
		)
		object.InputEnded:Connect(
			function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					_G.YUKIIXDRAG[object].mouseDown = false
					_G.YUKIIXDRAG[object].mmoved:Disconnect()
					if _G.YUKIIXDRAG[object].mloo then
						_G.YUKIIXDRAG[object].mloo:Disconnect()
					end
				end
			end
		)
	end
)
object.MouseLeave:Connect(
	function()
		if _G.YUKIIXDRAG[object].mdwn then
			_G.YUKIIXDRAG[object].mdwn:Disconnect()
		end
	end
)
end

