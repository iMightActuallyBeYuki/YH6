		if SelectedTab == 1 then
			PlayerBackground.Visible = false
			MiscBackground.Visible = false
			AboutBackground.Visible = false
			ScriptsBackground.Visible = false
			MainBackground.Visible = true
			ScriptsText.TextColor3 = Color3.fromRGB(57, 57, 57)
			PlayerText.TextColor3 = Color3.fromRGB(57, 57, 57)
			MiscText.TextColor3 = Color3.fromRGB(57, 57, 57)
			AboutText.TextColor3 = Color3.fromRGB(57, 57, 57)
			MainText.TextColor3 = Color3.fromRGB(255, 255, 255)
		end

		function Tab1()
			if SelectedTab == 1 then
				PlayerBackground.Visible = false
				MiscBackground.Visible = false
				AboutBackground.Visible = false
				ScriptsBackground.Visible = false
				MainBackground.Visible = true
				ScriptsText.TextColor3 = Color3.fromRGB(57, 57, 57)
				PlayerText.TextColor3 = Color3.fromRGB(57, 57, 57)
				MiscText.TextColor3 = Color3.fromRGB(57, 57, 57)
				AboutText.TextColor3 = Color3.fromRGB(57, 57, 57)
				MainText.TextColor3 = Color3.fromRGB(255, 255, 255)
			end
			UpdateSaves()
		end

		MainText.MouseButton1Click:Connect(
			function()
				SelectedTab = 1
				Tab1()
			end
		)

		if SelectedTab == 2 then
			MainBackground.Visible = false
			ScriptsBackground.Visible = false
			MiscBackground.Visible = false
			AboutBackground.Visible = false
			PlayerBackground.Visible = true
			MainText.TextColor3 = Color3.fromRGB(57, 57, 57)
			ScriptsText.TextColor3 = Color3.fromRGB(57, 57, 57)
			MiscText.TextColor3 = Color3.fromRGB(57, 57, 57)
			AboutText.TextColor3 = Color3.fromRGB(57, 57, 57)
			PlayerText.TextColor3 = Color3.fromRGB(255, 255, 255)
		end

		function Tab2()
