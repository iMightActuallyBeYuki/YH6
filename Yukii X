		BulletPunch.BorderSizePixel = 0
		BulletPunch.Position = UDim2.new(0.0428313538, 0, 0.67963165, 0)
		BulletPunch.Size = UDim2.new(0.257314742, 0, 0.253317267, 0)
		BulletPunch.Font = Enum.Font.SourceSansSemibold
		BulletPunch.Text = "Bullet Punch"
		BulletPunch.TextColor3 = Color3.fromRGB(255, 255, 255)
		BulletPunch.TextScaled = true
		BulletPunch.TextSize = 14.000
		BulletPunch.TextWrapped = true

		local LBPT = false

		if BP == 1 then
			BulletPunch.Text = "Bullet Punch"
		end

		if BP == 2 then
			BulletPunch.Text = "Loop Bullet Punch"
		end

		BulletPunch.MouseButton1Click:Connect(function()
			if PlayerChosen then
			if BP == 1 then
				local args = {
					[1] = {
						[1] = "Skill_BulletPunch",
						[2] = "Right",
						[3] = PlayerChosen.Character.HumanoidRootPart.Position
					}
				}
				game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
				wait()
				end
			if BP == 2 then
				LBPT = not LBPT
				while LBPT == true do
					BulletPunch.BackgroundColor3 = Color3.fromRGB(0,255,0)
					BulletPunch.Text = "Loop Bullet Punch (ON)"
					local args = {
						[1] = {
							[1] = "Skill_BulletPunch",
							[2] = "Right",
							[3] = PlayerChosen.Character.HumanoidRootPart.Position
						}
					}
					game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
					wait()
				end
				if LBPT == false then
					BulletPunch.BackgroundColor3 = Color3.fromRGB(68, 68, 68)
					BulletPunch.Text = "Loop Bullet Punch (OFF)"
				end
			end
		end
		end)

		BulletPunch.MouseButton2Click:connect(
			function()
				BulletPunchInfo.Visible = true
			end
		)

		BulletPunchInfo.MouseEnter:connect(
			function()
				BulletPunchInfo.Visible = true
			end
		)

		BulletPunchInfo.MouseLeave:connect(
			function()
				BulletPunchInfo.Visible = false
			end
		)

